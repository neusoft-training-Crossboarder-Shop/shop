  <select id="selectStoStrProByMany" parameterType="neu.train.project.order.vo.GetAStoOrderQueryByTime" resultMap="sendASimpleSto">
    select sto.sto_id,sto.qty,sto.purchase_price,sto.paid_time,sto.order_status,sto.create_time,sto.last_update_time,pro.pro_id,pro.title,pro.retail_price,str.store_id,str.platform_type,str.store_name
    from sto_store_order sto,str_store str,pro_product pro
    <where>
      <if test="bvoId != null and bvoId != '' and bvoId!=0">
        AND str.dsr_id = #{bvoId}
      <if test="startTime != null and startTime != ''"><!-- 开始时间检索 -->
        and date_format(sto.create_time,'%y%m%d') &gt;= date_format(#{startTime},'%y%m%d')
      </if>
      <if test="endTime != null and endTime != ''"><!-- 结束时间检索 -->
        and date_format(sto.create_time,'%y%m%d') &lt;= date_format(#{endTime},'%y%m%d')
      </if>
      and str.store_id=sto.store_id
      and pro.pro_id=sto.proId
    </where>
    order by sto.create_time desc
  </select>






